{
  "name": "@jupyterlab-nbgallery/environment-life",
  "version": "0.2.3",
  "description": "Allow an expiration data to be displayed in the bottom right corner when operating in an environment where jupyter instances have a defined lifetime.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/nbgallery/lab-extensions",
  "bugs": {
    "url": "https://github.com/nbgallery/lab-extensions/issues"
  },
  "license": "MIT",
  "author": "Team@NBG",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/nbgallery/lab-extensions"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib":"tsc",
    "build:all": "jlpm run build:prod",
    "build:ext": "jlpm run build:prod",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_nbgallery/labextension",
    "clean:all": "rimrafjlpm run clean:lib && jlpm run clean:labextension",
    "install-ext": "jlpm run build",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "watch:src": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": ">=3.1.0",
    "@jupyterlab/settingregistry": ">=3.1.0",
    "jquery":"^3.5.0"
  },
  "devDependencies": {
    "@jupyterlab/builder":">=3.3.2",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@types/jquery":"^3.5.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsdoc": "^40.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.18.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "outputDir": "../labextension/environment-life"
  }
}
